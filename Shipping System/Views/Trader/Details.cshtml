@model TraderVM;

<div class="col-12 grid-margin">
    <div class="card">
        <div class="card-body">
            <p class="card-description"> بيانات التاجر </p>
            <div class="row">
                <div class="col-md-4">
                    <h3>الاسم : @Model.FullName</h3>
                </div>
                <div class="col-md-4">
                    <h3>العنوان : @Model.Address</h3>
                </div>
                <div class="col-md-4">
                    <h3>المحافظة : @Model.GovernateName</h3>
                </div>
                <div class="col-md-4">
                    <h3>المدينة : @Model.CityName</h3>
                </div>
                <div class="col-md-4">
                    <h3>الـفـرع : @Model.BranchName</h3>
                </div>
                <div class="col-md-4">
                    <h3>اسم المستخدم : @Model.UserName</h3>
                </div>
                <div class="col-md-4">
                    <h3>البريد الالكتروني : @Model.Email</h3>
                </div>
                <div class="col-md-4">
                    <h3>رقم الهاتف : @Model.PhoneNumber</h3>
                </div>
                <div class="col-md-4">
                    <h3>نسبة تحمل التاجر لقيمة شحن الطلبات المرفوضه : @Model.Trader_RejOrderPrec</h3>
                </div>

            </div>
        </div>

    </div>
</div>
